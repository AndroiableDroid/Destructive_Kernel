			<&msm_gpio 33 0>,
			<&msm_gpio 1 0>,
            <&msm_gpio 21 0>;//9 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-standby = <2>;
	    qcom,gpio-vdig = <3>;
		qcom,gpio-vana = <4>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK1",
    		"SCAM_RESET",
    		"SCAM_STANDBY",
    		"SCAM_GPIO_DVDD",
    		"SCAM_GPIO_AVDD";

        qcom,cam-power-seq-type = "sensor_vreg",
            "sensor_gpio",
            "sensor_gpio",
            "sensor_gpio",
            "sensor_gpio",
            "sensor_clk";
		qcom,cam-power-seq-val = "cam_vio",
            "sensor_gpio_vana",
            "sensor_gpio_vdig",
            "sensor_gpio_reset",
